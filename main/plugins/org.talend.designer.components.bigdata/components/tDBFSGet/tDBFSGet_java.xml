<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true" PARTITIONING="AUTO" LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>
	<FAMILIES>
		<FAMILY>Big Data/DBFS</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" NUM_ROW="1"
			REPOSITORY_VALUE="DBFS" />
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			FILTER="tDBFSConnection" NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true" />
			
		<PARAMETER NAME="DATABRICKS_ENDPOINT" REPOSITORY_VALUE="DATABRICKS_ENDPOINT"
			FIELD="TEXT" NUM_ROW="10"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>"https://westeurope.azuredatabricks.net"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DATABRICKS_TOKEN" REPOSITORY_VALUE="DATABRICKS_TOKEN"
			FIELD="PASSWORD" REQUIRED="true" NUM_ROW="15"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DATABRICKS_DBFS_FOLDER" REPOSITORY_VALUE="DATABRICKS_DBFS_FOLDER"
			FIELD="TEXT" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="LOCALDIR" FIELD="DIRECTORY" REQUIRED="false"
			NUM_ROW="25">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="OVERWRITE" FIELD="CLOSED_LIST" NUM_ROW="30">
			<ITEMS DEFAULT="false">
				<ITEM NAME="NEVER" VALUE="false" />
				<ITEM NAME="ALWAYS" VALUE="true" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="INCLUDSUBDIR" FIELD="CHECK" NUM_ROW="35">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="FILES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="40" NB_LINES="5">
			<ITEMS>
				<ITEM NAME="FILEMASK" />
				<ITEM NAME="NEWNAME" VALUE="&quot;&quot;" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="120">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
	</PARAMETERS>
	<ADVANCED_PARAMETERS>

	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS REQUIRED_IF="(USE_EXISTING_CONNECTION=='false')">

           <IMPORT NAME="reload4j-1.2.19" MODULE="reload4j-1.2.19.jar" 
                   MVN="mvn:ch.qos.reload4j/reload4j/1.2.19"/>

           <IMPORT NAME="commons-logging-1.2.jar" MODULE="commons-logging-1.2.jar" 
                   MVN="mvn:commons-logging/commons-logging/1.2"/>

           <IMPORT NAME="talend-bigdata-launcher-databricks-2.1.0.jar"
                   MODULE="talend-bigdata-launcher-databricks-2.1.0.jar"
                   MVN="mvn:org.talend.bigdata.libs/talend-bigdata-launcher-databricks/2.1.0"/>

           <IMPORT NAME="talend-bigdata-launcher-jobserver-2.0.12.jar"
                   MODULE="talend-bigdata-launcher-jobserver-2.0.12.jar"
                   MVN="mvn:org.talend.bigdata.libs/talend-bigdata-launcher-jobserver/2.0.12"/>

           <IMPORT NAME="gson-2.2.4.jar" MODULE="gson-2.2.4.jar" 
                   MVN="mvn:com.google.code.gson/gson/2.2.4"/>

		</IMPORTS>

	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_FILE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="CURRENT_STATUS" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="TRANSFER_MESSAGES" TYPE="id_String" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>