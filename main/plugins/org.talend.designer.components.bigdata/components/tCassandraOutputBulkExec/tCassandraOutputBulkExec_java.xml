<COMPONENT>
    <HEADER
        AUTHOR="Talend"
        COMPATIBILITY="ALL"
        DATA_AUTO_PROPAGATE="true"
        PLATEFORM="ALL"
        RELEASE_DATE="20080229A"
        SERIAL=""
        STATUS="ALPHA"
        VERSION="0.102"
        STARTABLE="false"
    >
        <SIGNATURE />
    </HEADER>
    <FAMILIES>
        <FAMILY>Databases NoSQL/Cassandra</FAMILY>
        <FAMILY>Big Data/Cassandra</FAMILY>
    </FAMILIES>
    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>
    <CONNECTORS>
        <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
        <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
        <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
        <CONNECTOR CTYPE="COMPONENT_OK"/>
        <CONNECTOR CTYPE="COMPONENT_ERROR"/>
        <CONNECTOR CTYPE="RUN_IF"/>
    </CONNECTORS>
    <PARAMETERS>
        <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" NUM_ROW="1" REPOSITORY_VALUE="NOSQL:CASSANDRA"/>
        <PARAMETER
            NAME="SCHEMA_FLOW"
            FIELD="SCHEMA_TYPE"
            NUM_ROW="3"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="MAPPING"
            FIELD="MAPPING_TYPE"
            NUM_ROW="3"
            SHOW="false"
        >
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_0_0')">cassandra_id</DEFAULT>
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_1')">cassandra_datastax_id</DEFAULT>
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_2') OR (DB_VERSION == 'CASSANDRA_3_0')">cassandra22_datastax_id</DEFAULT>
			<DEFAULT IF="(DB_VERSION == 'CASSANDRA_4_0')">cassandra40_datastax_id</DEFAULT>
        </PARAMETER>
        <!-- AFTER 2.1 only keep CQL type -->
        <PARAMETER
            NAME="TABLE_TYPE"
            FIELD="CLOSED_LIST"
            NUM_ROW="4"
            SHOW_IF="(DB_VERSION == 'CASSANDRA_2_0_0')"
        >
            <ITEMS DEFAULT="NON_CQL">
                <ITEM NAME="NON_CQL" VALUE="NON_CQL"/>
                <ITEM NAME="CQL" VALUE="CQL"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="DB_VERSION"
            FIELD="CLOSED_LIST"
            REPOSITORY_VALUE="DB_VERSION"
            NUM_ROW="5"
            >
            <ITEMS DEFAULT="CASSANDRA_4_0">
                <ITEM NAME="CASSANDRA_3_0" VALUE="CASSANDRA_3_0"/>
                <ITEM NAME="CASSANDRA_4_0" VALUE="CASSANDRA_4_0"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="HOST"
            FIELD="TEXT"
            REPOSITORY_VALUE="HOST"
            REQUIRED="true"
            NUM_ROW="10"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="PORT"
            FIELD="TEXT"
            REPOSITORY_VALUE="PORT"
            REQUIRED="true"
            NUM_ROW="10"
        >
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_0_0') OR (DB_VERSION == 'CASSANDRA_2_1')">9160</DEFAULT>
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_2') OR (DB_VERSION == 'CASSANDRA_3_0') OR (DB_VERSION == 'CASSANDRA_4_0')">9042</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="REQUIRED_AUTHENTICATION"
            FIELD="CHECK"
            REPOSITORY_VALUE="REQUIRED_AUTHENTICATION"
            NUM_ROW="11"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="USERNAME"
            FIELD="TEXT"
            REPOSITORY_VALUE="USERNAME"
            REQUIRED="true"
            NUM_ROW="12"
            SHOW_IF="(isShow[REQUIRED_AUTHENTICATION]) AND (REQUIRED_AUTHENTICATION == 'true')"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="PASSWORD"
            FIELD="PASSWORD"
            REPOSITORY_VALUE="PASSWORD"
            REQUIRED="true"
            NUM_ROW="12"
            SHOW_IF="(isShow[REQUIRED_AUTHENTICATION]) AND (REQUIRED_AUTHENTICATION == 'true')"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="USE_HTTPS" FIELD="CHECK" NUM_ROW="22" SHOW="false">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="HTTPS_SETTING" FIELD="COMPONENT_LIST" FILTER="tSetKeystore"
            NUM_ROW="22" SHOW_IF="(isShow[USE_HTTPS]) AND (USE_HTTPS == 'true')"
            DYNAMIC_SETTINGS="true" />
        <PARAMETER NAME="USE_CONF_FILE" FIELD="CHECK" NUM_ROW="23" NOT_SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>            
        <PARAMETER
            NAME="CONF_FILE"
            FIELD="FILE"
            REQUIRED="true"
            NUM_ROW="23"
            SHOW_IF="(isShow[USE_CONF_FILE]) AND USE_CONF_FILE == 'true'"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER> 
        <PARAMETER
            NAME="KEY_SPACE"
            FIELD="TEXT"
            REPOSITORY_VALUE="KEY_SPACE"
            REQUIRED="true"
            NUM_ROW="25"
			NOT_SHOW_IF="((DB_VERSION=='CASSANDRA_4_0') AND (USE_STATEMENT=='true'))"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="COLUMN_FAMILY"
            FIELD="TEXT"
            REQUIRED="true"
            NUM_ROW="25"
			NOT_SHOW_IF="((DB_VERSION=='CASSANDRA_4_0') AND (USE_STATEMENT=='true'))"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="PARTITIONER"
            FIELD="CLOSED_LIST"
            REQUIRED="true"
            NUM_ROW="25"
			NOT_SHOW_IF="(DB_VERSION=='CASSANDRA_4_0')"
        >
            <ITEMS DEFAULT="RANDOMPARTITIONER">
                <ITEM NAME="RANDOMPARTITIONER" VALUE="RandomPartitioner"/>
                <ITEM NAME="MURMUR3PARTITIONER" VALUE="Murmur3Partitioner"/>
                <ITEM NAME="ORDERPRESERVINGPARTITIONER" VALUE="OrderPreservingPartitioner"/>
            </ITEMS>
        </PARAMETER>
		
		<PARAMETER
			NAME="STMT_MAPPING_TYPE"
			FIELD="CLOSED_LIST"
			NUM_ROW="28"
			REQUIRED="true"
            SHOW_IF="((DB_VERSION == 'CASSANDRA_4_0'))"
		>
			<ITEMS DEFAULT="NONE">
                <ITEM NAME="NONE" VALUE="NONE"/>
				<ITEM NAME="AUTO_GENERATE" VALUE="AUTO_GENERATE"/>
                <ITEM NAME="MANUAL" VALUE="MANUAL"/>
            </ITEMS>
		</PARAMETER>
		<PARAMETER
            NAME="STMT_MAPPING"
            FIELD="TEXT"
            NUM_ROW="29"
            REQUIRED="true"
            SHOW_IF="((DB_VERSION == 'CASSANDRA_4_0') AND (STMT_MAPPING_TYPE=='MANUAL'))"
        >
          <DEFAULT>"0=id, 1=name, 2=birthday"</DEFAULT>
        </PARAMETER>
		<PARAMETER
            NAME="DATA_FILE_PATH"
            FIELD="FILE"
            REQUIRED="true"
            NUM_ROW="34"
			SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
        <PARAMETER
            NAME="COMPARATOR"
            FIELD="CLOSED_LIST"
            REQUIRED="true"
            NUM_ROW="30"
            SHOW_IF="((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL'))"
        >
            <ITEMS DEFAULT="ASCIITYPE">
                <ITEM NAME="ASCIITYPE" VALUE="AsciiType"/>
                <ITEM NAME="BYTESTYPE" VALUE="BytesType"/>
                <ITEM NAME="LEXICALUUIDTYPE" VALUE="LexicalUUIDType"/>
                <ITEM NAME="LONGTYPE" VALUE="LongType"/>
                <ITEM NAME="TIMEUUIDTYPE" VALUE="TimeUUIDType"/>
                <ITEM NAME="UTF8TYPE" VALUE="UTF8Type"/>
            </ITEMS>
        </PARAMETER>
        <!--PARAMETER
            NAME="SUBCOMPARATOR"
            FIELD="CLOSED_LIST"
            REQUIRED="true"
            NUM_ROW="25"
        >
            <ITEMS DEFAULT="NONE">
                <ITEM NAME="NONE" VALUE="NONE"/>
                <ITEM NAME="ASCIITYPE" VALUE="ASCIITYPE"/>
            </ITEMS>
        </PARAMETER-->
        <PARAMETER
            NAME="SCHEMA_STMT"
            FIELD="TEXT"
            NUM_ROW="26"
            REQUIRED="true"
            NOT_SHOW_IF="((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL')) OR (DB_VERSION == 'CASSANDRA_4_0')"
        >
          <DEFAULT>"create table ks.tb (id int, name text, birthday timestamp, primary key(id, birthday)) with clustering order by (birthday desc)"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="SSTABLE_STORE_PATH"
            FIELD="DIRECTORY"
            REQUIRED="true"
            NUM_ROW="40"
			NOT_SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="BUFFER_SIZE"
            FIELD="TEXT"
            REQUIRED="true"
            NUM_ROW="45"
			NOT_SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')"
        >
            <DEFAULT IF="((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL'))">64</DEFAULT>
            <DEFAULT IF="((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='CQL')) OR (DB_VERSION == 'CASSANDRA_2_1') OR (DB_VERSION == 'CASSANDRA_2_2') OR (DB_VERSION == 'CASSANDRA_3_0')">128</DEFAULT>
        </PARAMETER>
    </PARAMETERS>
    <ADVANCED_PARAMETERS>
		<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="30" REPOSITORY_VALUE="ROW_SEPARATOR" SHOW="false">
		  <DEFAULT>"\n"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="30" REPOSITORY_VALUE="FIELD_SEPARATOR" SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')">
		  <DEFAULT>","</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ESCAPE_CHAR" FIELD="TEXT" REQUIRED="true" NUM_ROW="30" MAX_LENGTH="1" REPOSITORY_VALUE="ESCAPE_CHAR" SHOW="false">
		  <DEFAULT>"\\"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" REQUIRED="true" NUM_ROW="30" MAX_LENGTH="1" REPOSITORY_VALUE="TEXT_ENCLOSURE" SHOW="false">
		  <DEFAULT>"\""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true" NUM_ROW="40" SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')">
		  <DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="50" REQUIRED="true" REPOSITORY_VALUE="ENCODING" SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')">
		  <DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="USE_STATEMENT"
			FIELD="CHECK"
			NUM_ROW="51"
			REQUIRED="true"
            SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
        <PARAMETER
            NAME="STMT"
            FIELD="TEXT"
            NUM_ROW="52"
            REQUIRED="true"
            SHOW_IF="(isShow[USE_STATEMENT] AND (USE_STATEMENT=='true'))"
        >
          <DEFAULT>"insert into ks.tb (id, name, birthday) values (?, ?, ?)"</DEFAULT>
        </PARAMETER>
		<PARAMETER
			NAME="USE_HEADER"
			FIELD="CHECK"
			NUM_ROW="55"
			REQUIRED="true"
            SHOW_IF="(DB_VERSION == 'CASSANDRA_4_0')"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
    </ADVANCED_PARAMETERS>
    <CODEGENERATION>
        <TEMPLATES INPUT="tCOB" OUTPUT="tCBE">
            <TEMPLATE NAME="tCOB" COMPONENT="tCassandraOutputBulk">
              <LINK_TO NAME="tCBE" CTYPE="ROWS_END"/>
            </TEMPLATE>

            <TEMPLATE NAME="tCBE" COMPONENT="tCassandraBulkExec"/>

            <TEMPLATE_PARAM SOURCE="self.SCHEMA_FLOW" TARGET="tCOB.SCHEMA_FLOW"/>
            <TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="tCOB.DB_VERSION"/>
            <TEMPLATE_PARAM SOURCE="self.KEY_SPACE" TARGET="tCOB.KEY_SPACE"/>
            <TEMPLATE_PARAM SOURCE="self.COLUMN_FAMILY" TARGET="tCOB.COLUMN_FAMILY"/>
            <TEMPLATE_PARAM SOURCE="self.PARTITIONER" TARGET="tCOB.PARTITIONER"/>
            <TEMPLATE_PARAM SOURCE="self.COMPARATOR" TARGET="tCOB.COMPARATOR"/>
            <TEMPLATE_PARAM SOURCE="self.BUFFER_SIZE" TARGET="tCOB.BUFFER_SIZE"/>
            <TEMPLATE_PARAM SOURCE="self.SSTABLE_STORE_PATH" TARGET="tCOB.SSTABLE_STORE_PATH"/>
            <TEMPLATE_PARAM SOURCE="self.TABLE_TYPE" TARGET="tCOB.TABLE_TYPE"/>
            <TEMPLATE_PARAM SOURCE="self.SCHEMA_STMT" TARGET="tCOB.SCHEMA_STMT"/>

            <TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="tCBE.DB_VERSION"/>
            <TEMPLATE_PARAM SOURCE="self.HOST" TARGET="tCBE.HOST"/>
            <TEMPLATE_PARAM SOURCE="self.PORT" TARGET="tCBE.PORT"/>
            <TEMPLATE_PARAM SOURCE="self.REQUIRED_AUTHENTICATION" TARGET="tCBE.REQUIRED_AUTHENTICATION"/>
            <TEMPLATE_PARAM SOURCE="self.USERNAME" TARGET="tCBE.USERNAME"/>
            <TEMPLATE_PARAM SOURCE="self.PASSWORD" TARGET="tCBE.PASSWORD"/>
            <TEMPLATE_PARAM SOURCE="self.USE_HTTPS" TARGET="tCBE.USE_HTTPS"/>
            <TEMPLATE_PARAM SOURCE="self.HTTPS_SETTING" TARGET="tCBE.HTTPS_SETTING"/>
            <TEMPLATE_PARAM SOURCE="self.USE_CONF_FILE" TARGET="tCBE.USE_CONF_FILE"/>
            <TEMPLATE_PARAM SOURCE="self.CONF_FILE" TARGET="tCBE.CONF_FILE"/>
            <TEMPLATE_PARAM SOURCE="self.KEY_SPACE" TARGET="tCBE.KEY_SPACE"/>
            <TEMPLATE_PARAM SOURCE="self.COLUMN_FAMILY" TARGET="tCBE.COLUMN_FAMILY"/>
            <TEMPLATE_PARAM SOURCE="self.SSTABLE_STORE_PATH" TARGET="tCBE.SSTABLE_STORE_PATH"/>
			
			<TEMPLATE_PARAM SOURCE="self.USE_STATEMENT" TARGET="tCBE.USE_STATEMENT"/>
			<TEMPLATE_PARAM SOURCE="self.USE_STATEMENT" TARGET="tCOB.USE_STATEMENT"/>
			<TEMPLATE_PARAM SOURCE="self.STMT" TARGET="tCBE.STMT"/>
			<TEMPLATE_PARAM SOURCE="self.STMT" TARGET="tCOB.INSERT_STMT"/>
			<TEMPLATE_PARAM SOURCE="self.STMT_MAPPING_TYPE" TARGET="tCBE.STMT_MAPPING_TYPE"/>
			<TEMPLATE_PARAM SOURCE="self.STMT_MAPPING_TYPE" TARGET="tCOB.STMT_MAPPING_TYPE"/>
			<TEMPLATE_PARAM SOURCE="self.STMT_MAPPING" TARGET="tCBE.STMT_MAPPING"/>
			<TEMPLATE_PARAM SOURCE="self.STMT_MAPPING" TARGET="tCOB.STMT_MAPPING"/>
			<TEMPLATE_PARAM SOURCE="self.USE_HEADER" TARGET="tCBE.USE_HEADER"/>
			<TEMPLATE_PARAM SOURCE="self.USE_HEADER" TARGET="tCOB.USE_HEADER"/>
			<TEMPLATE_PARAM SOURCE="self.DATA_FILE_PATH" TARGET="tCBE.DATA_FILE_PATH"/>
			<TEMPLATE_PARAM SOURCE="self.DATA_FILE_PATH" TARGET="tCOB.DATA_FILE_PATH"/>
			<TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tCBE.ROWSEPARATOR"/>
			<TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tCOB.ROWSEPARATOR"/>
			<TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR" TARGET="tCBE.FIELDSEPARATOR"/>
			<TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR" TARGET="tCOB.FIELDSEPARATOR"/>
			<TEMPLATE_PARAM SOURCE="self.ESCAPE_CHAR" TARGET="tCBE.ESCAPE_CHAR"/>
			<TEMPLATE_PARAM SOURCE="self.ESCAPE_CHAR" TARGET="tCOB.ESCAPE_CHAR"/>
			<TEMPLATE_PARAM SOURCE="self.CREATE" TARGET="tCBE.CREATE"/>
			<TEMPLATE_PARAM SOURCE="self.CREATE" TARGET="tCOB.CREATE"/>
			<TEMPLATE_PARAM SOURCE="self.TEXT_ENCLOSURE" TARGET="tCBE.TEXT_ENCLOSURE"/>
			<TEMPLATE_PARAM SOURCE="self.TEXT_ENCLOSURE" TARGET="tCOB.TEXT_ENCLOSURE"/>
			<TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tCBE.ENCODING"/>
			<TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tCOB.ENCODING"/>
			
        </TEMPLATES>
    </CODEGENERATION>
    <RETURNS/>
</COMPONENT>
