<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true" LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>
	<FAMILIES>
		<FAMILY>File/Hadoop</FAMILY>
		<FAMILY>Big Data/HDFS</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" NUM_ROW="1"
			REPOSITORY_VALUE="HDFS" />
		<PARAMETER NAME="HDFS" FIELD="HADOOP_LIBRARIES"
			NUM_ROW="2" SHOW="false" />
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="3">
			<DEFAULT />
		</PARAMETER>
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tHDFSConnection" NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true" />

		<PARAMETER NAME="HDFS" FIELD="HADOOP_DISTRIBUTION" NUM_ROW="8"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false')" GROUP="VERSION" />

		<PARAMETER NAME="HADOOP_CUSTOM_JARS" GROUP="VERSION"
			FIELD="HADOOP_JARS_DIALOG" NUM_ROW="8"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DISTRIBUTION=='CUSTOM')"
			REPOSITORY_VALUE="HADOOP_CUSTOM_JARS">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="SCHEME" FIELD="CLOSED_LIST"
			NUM_ROW="9" GROUP="SCHEME" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')" REPOSITORY_VALUE="SCHEME">
			<ITEMS DEFAULT="HDFS">
				<ITEM NAME="HDFS" VALUE="HDFS" />
				<ITEM NAME="ADLS" VALUE="ADLS" SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportAzureDataLakeStorage[])" />
				<ITEM NAME="WebHDFS" VALUE="WebHDFS" SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportWebHDFS[])" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="AUTHENTICATION_MODE" FIELD="CLOSED_LIST"
			NUM_ROW="11"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DISTRIBUTION=='CUSTOM')"
			REPOSITORY_VALUE="AUTHENTICATION_MODE" GROUP="CUSTOM_COMMON_SETTINGS">
			<ITEMS DEFAULT="USERNAME">
				<ITEM NAME="USERNAME" VALUE="USERNAME" />
				<ITEM NAME="KRB" VALUE="KRB" />
				<ITEM NAME="UGI" VALUE="UGI" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="FS_DEFAULT_NAME" FIELD="TEXT"
			REPOSITORY_VALUE="FS_DEFAULT_NAME" REQUIRED="true" NUM_ROW="20"
			SHOW_IF="(SCHEME!='ADLS') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="CONN">
			<DEFAULT>"hdfs://localhost:9000/"</DEFAULT>
			<DEFAULT IF="(DISTRIBUTION=='MAPR')">"maprfs:///"</DEFAULT>
			<DEFAULT IF="(SCHEME=='HDFS')">"hdfs://localhost:9000/"</DEFAULT>
			<DEFAULT IF="(SCHEME=='WebHDFS')">"webhdfs://localhost:50070/"</DEFAULT>
		</PARAMETER>

		<!-- ADLS params -->
		<PARAMETER NAME="ADLS_FS_DEFAULT_NAME" FIELD="TEXT"
			REPOSITORY_VALUE="AZURE_FS_DEFAULT_NAME" REQUIRED="true" NUM_ROW="20"
			SHOW_IF="(SCHEME=='ADLS') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="ADLS">
			<DEFAULT>"adl://account.azuredatalakestore.net"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ADLS_CLIENT_ID" FIELD="TEXT" NUM_ROW="21" REQUIRED="true"
			SHOW_IF="(SCHEME=='ADLS') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="ADLS">
			<DEFAULT>"XXXX"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ADLS_AUTH_TOKEN_ENDPOINT" FIELD="PASSWORD" NUM_ROW="22" REQUIRED="true"
			SHOW_IF="(SCHEME=='ADLS') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="ADLS">
			<DEFAULT>"https://login.microsoftonline.com/XXX/oauth2/token"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ADLS_CLIENT_KEY" FIELD="PASSWORD" NUM_ROW="23" REQUIRED="true"
			SHOW_IF="(SCHEME=='ADLS') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="ADLS">
			<DEFAULT>"XXXX"</DEFAULT>
		</PARAMETER>
		<!-- ADLS params -->

		<PARAMETER NAME="USE_DATANODE_HOSTNAME" FIELD="CHECK"
			REPOSITORY_VALUE="USE_DATANODE_HOSTNAME" NUM_ROW="21" REQUIRED="true"
			GROUP="CONN"
			SHOW_IF="(SCHEME!='ADLS') AND ((USE_EXISTING_CONNECTION == 'false') AND DISTRIB[DISTRIBUTION, DB_VERSION].doSupportUseDatanodeHostname[]))">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_KRB" FIELD="CHECK" NUM_ROW="25"
			REPOSITORY_VALUE="USE_KRB"
			SHOW_IF="(SCHEME!='ADLS') AND (isShow[DISTRIBUTION] AND (DISTRIB[DISTRIBUTION, DB_VERSION].doSupportKerberos[]))"
			GROUP="AUTHENTICATION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NAMENODE_PRINCIPAL" FIELD="TEXT"
			REPOSITORY_VALUE="NAMENODE_PRINCIPAL" REQUIRED="true" NUM_ROW="26"
			SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_KRB] AND (USE_KRB=='true')) OR (isShow[AUTHENTICATION_MODE] AND (AUTHENTICATION_MODE=='KRB')))"
			GROUP="AUTHENTICATION">
			<DEFAULT>"nn/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_KEYTAB" REPOSITORY_VALUE="USE_KEYTAB"
			FIELD="CHECK" NUM_ROW="27"
			SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_KRB] AND (USE_KRB=='true')) OR (isShow[AUTHENTICATION_MODE] AND (AUTHENTICATION_MODE=='KRB')))"
			GROUP="AUTHENTICATION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PRINCIPAL" REPOSITORY_VALUE="KEYTAB_PRINCIPAL"
			FIELD="TEXT" REQUIRED="true" NUM_ROW="28"
			SHOW_IF="(SCHEME!='ADLS') AND (isShow[USE_KEYTAB] AND (USE_KEYTAB=='true'))" GROUP="AUTHENTICATION">
			<DEFAULT>"hdfs"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="KEYTAB_PATH" REPOSITORY_VALUE="KEYTAB_PATH"
			FIELD="FILE" REQUIRED="true" NUM_ROW="28"
			SHOW_IF="(SCHEME!='ADLS') AND (isShow[USE_KEYTAB] AND (USE_KEYTAB=='true'))" GROUP="AUTHENTICATION">
			<DEFAULT>"/tmp/hdfs.headless.keytab"</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="USE_MAPRTICKET" FIELD="CHECK" NUM_ROW="29"
            REPOSITORY_VALUE="USE_MAPRTICKET" GROUP="AUTHENTICATION"
            SHOW_IF="(SCHEME!='ADLS') AND (DISTRIB[DISTRIBUTION, DB_VERSION].doSupportMapRTicket[])">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" REPOSITORY_VALUE="USERNAME"
			REQUIRED="true" NUM_ROW="30"
			NOT_SHOW_IF="(SCHEME=='ADLS') OR (USE_EXISTING_CONNECTION == 'true') OR (isShow[USE_KRB] AND (USE_KRB=='true')) OR (isShow[AUTHENTICATION_MODE] AND (AUTHENTICATION_MODE=='KRB' OR AUTHENTICATION_MODE=='UGI')) OR ((DISTRIBUTION=='MAPR') AND ((!isShow[USE_MAPRTICKET]) OR (USE_MAPRTICKET=='false')))"
			GROUP="AUTHENTICATION">
			<DEFAULT>"anonymous"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="GROUP" FIELD="TEXT" REPOSITORY_VALUE="GROUP"
			REQUIRED="true" NUM_ROW="30"
			SHOW_IF="(SCHEME!='ADLS') AND (USE_EXISTING_CONNECTION == 'false') AND (DISTRIB[DISTRIBUTION, DB_VERSION].doSupportGroup[])"
			GROUP="AUTHENTICATION">
			<DEFAULT>"supergroup"</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="MAPRTICKET_PASSWORD" FIELD="PASSWORD" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPRTICKET_PASSWORD" REQUIRED="true" NUM_ROW="32"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true')) AND (((isShow[USE_KRB]) AND (USE_KRB=='false')) OR (!isShow[USE_KRB])) ">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPRTICKET_CLUSTER" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPRTICKET_CLUSTER" REQUIRED="true" NUM_ROW="33"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
            <DEFAULT>"demo.mapr.com"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPRTICKET_DURATION" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPRTICKET_DURATION" REQUIRED="true" NUM_ROW="34"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
            <DEFAULT>86400L</DEFAULT>
        </PARAMETER>

		<PARAMETER NAME="FILENAME" FIELD="BOTH_HADOOP" NUM_ROW="40"
			REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TYPEFILE" FIELD="CLOSED_LIST" NUM_ROW="41"
			REQUIRED="true" GROUP="FILE_TYPE">
			<ITEMS DEFAULT="TEXT">
				<ITEM NAME="TEXT" VALUE="TEXT" />
				<ITEM NAME="SEQUENCE" VALUE="SEQUENCE" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="ROWSEPARATOR" REPOSITORY_VALUE="ROWSEPARATOR"
			SHOW_IF="TYPEFILE == 'TEXT'" FIELD="TEXT" NUM_ROW="50" REQUIRED="true">
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDSEPARATOR" REPOSITORY_VALUE="FIELDSEPARATOR"
			SHOW_IF="TYPEFILE == 'TEXT'" FIELD="TEXT" NUM_ROW="50" REQUIRED="true">
			<DEFAULT>";"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="HEADER" FIELD="TEXT" NUM_ROW="50"
			SHOW_IF="TYPEFILE == 'TEXT'" REPOSITORY_VALUE="HEADER">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CUSTOM_ENCODING" FIELD="CHECK" SHOW_IF="TYPEFILE == 'TEXT'"
			NUM_ROW="55" REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="60"
			REQUIRED="true" SHOW_IF="(CUSTOM_ENCODING == 'true' AND TYPEFILE == 'TEXT')">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="KEYCOLUMN" SHOW_IF="TYPEFILE == 'SEQUENCE'"
			FIELD="COLUMN_LIST" NUM_ROW="50" REQUIRED="true" GROUP="FILE_TYPE">
		</PARAMETER>
		<PARAMETER NAME="VALUECOLUMN" SHOW_IF="TYPEFILE == 'SEQUENCE'"
			FIELD="COLUMN_LIST" NUM_ROW="60" REQUIRED="true" GROUP="FILE_TYPE">
		</PARAMETER>
		<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="70"
			SHOW_IF="TYPEFILE == 'SEQUENCE'" GROUP="FILE_TYPE">
			<DEFAULT>NOTE : Only boolean, byte, byte[], double, float, integer,
				long, short, string are currently supported for key class and value
				columns fields
			</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="UNCOMPRESS" FIELD="CHECK" NUM_ROW="75"
			REQUIRED="true" GROUP="COMPRESSION_GROUP" SHOW_IF="TYPEFILE == 'TEXT'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="COMPRESSION" FIELD="CLOSED_LIST" NUM_ROW="75"
			GROUP="COMPRESSION_GROUP" SHOW_IF="(UNCOMPRESS=='true' AND TYPEFILE == 'TEXT')">
			<ITEMS DEFAULT="GZIP">
				<ITEM NAME="GZIP" VALUE="GZIP" />
				<ITEM NAME="BZIP2" VALUE="BZIP2" />
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="INCLUDSUBDIR" FIELD="CHECK" NUM_ROW="1">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="HADOOP_ADVANCED_PROPERTIES"
			REPOSITORY_VALUE="HADOOP_ADVANCED_PROPERTIES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="10" NB_LINES="3" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
				<ITEM NAME="BUILDIN" SHOW_IF="false" VALUE="TRUE" />
			</ITEMS>
		</PARAMETER>

        <PARAMETER NAME="SET_MAPR_HOME_DIR" FIELD="CHECK" NUM_ROW="50"
            REPOSITORY_VALUE="SET_MAPR_HOME_DIR" GROUP="AUTHENTICATION"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPR_HOME_DIR" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPR_HOME_DIR" REQUIRED="true" NUM_ROW="50"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[SET_MAPR_HOME_DIR]) AND (SET_MAPR_HOME_DIR=='true'))">
            <DEFAULT>"/opt/mapr"</DEFAULT>
        </PARAMETER>

       <PARAMETER NAME="SET_HADOOP_LOGIN" FIELD="CHECK" NUM_ROW="55"
            REPOSITORY_VALUE="SET_HADOOP_LOGIN" GROUP="AUTHENTICATION"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="HADOOP_LOGIN" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="HADOOP_LOGIN" REQUIRED="true" NUM_ROW="55"
            SHOW_IF="(SCHEME!='ADLS') AND ((isShow[SET_HADOOP_LOGIN]) AND (SET_HADOOP_LOGIN=='true'))">
            <DEFAULT>"kerberos"</DEFAULT>
        </PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="reload4j-1.2.19" MODULE="reload4j-1.2.19.jar"
					MVN="mvn:ch.qos.reload4j/reload4j/1.2.19"/>
			<IMPORT NAME="hadoop-conf-kerberos" MODULE="hadoop-conf-kerberos.jar"
				MVN="mvn:org.talend.libraries/hadoop-conf-kerberos/6.0.0"
				UrlPath="platform:/plugin/org.talend.libraries.hadoop.mapr2/lib/hadoop-conf-kerberos.jar"
				REQUIRED_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportKerberos[] AND (USE_KRB=='true') AND (USE_EXISTING_CONNECTION=='false') AND (DISTRIBUTION!='CUSTOM'))" />
			<IMPORT NAME="jetty-util" MODULE="jetty-util-6.1.26.jar"
				MVN="mvn:org.mortbay.jetty/jetty-util/6.1.26"
				REQUIRED_IF="DISTRIB[DISTRIBUTION, DB_VERSION].doSupportOldImportMode[] AND (DISTRIBUTION=='CLOUDERA') AND (USE_EXISTING_CONNECTION=='false') AND (DISTRIBUTION!='CUSTOM')" />
			<IMPORT NAME="commons-io" MODULE="commons-io-2.4.jar"
				MVN="mvn:commons-io/commons-io/2.4"
				UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-io-2.4.jar"
				REQUIRED_IF="DISTRIB[DISTRIBUTION, DB_VERSION].doSupportOldImportMode[] AND (DISTRIBUTION=='CLOUDERA') AND (USE_EXISTING_CONNECTION=='false') AND (DISTRIBUTION!='CUSTOM')" />

			<IMPORT NAME="Talend File Enhanced" MODULE="talend_file_enhanced-1.1.jar"
				MVN="mvn:org.talend.components.lib/talend_file_enhanced/1.1"
				UrlPath="platform:/plugin/org.talend.libraries.custom/lib/talend_file_enhanced-1.1.jar"
				REQUIRED="true"/>
		</IMPORTS>
