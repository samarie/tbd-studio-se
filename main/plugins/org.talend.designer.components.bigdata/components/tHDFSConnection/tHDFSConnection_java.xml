<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true" PARTITIONING="NONE" LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>
	<FAMILIES>
		<FAMILY>File/Hadoop</FAMILY>
		<FAMILY>Big Data/HDFS</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="HDFS" />
			
		<PARAMETER NAME="HDFS" FIELD="HADOOP_LIBRARIES"
			NUM_ROW="2" SHOW="false" />
			
		<PARAMETER NAME="HDFS" FIELD="HADOOP_DISTRIBUTION"
			NUM_ROW="5" GROUP="VERSION"/>

		<PARAMETER NAME="HADOOP_CUSTOM_JARS" GROUP="VERSION"
			FIELD="HADOOP_JARS_DIALOG" NUM_ROW="5" SHOW_IF="DISTRIBUTION=='CUSTOM'"
			REPOSITORY_VALUE="HADOOP_CUSTOM_JARS">
			<DEFAULT />
		</PARAMETER>
		
		<PARAMETER FIELD="LABEL" NAME="DEPRECATE_NOTE" COLOR="255;0;0"
                   NUM_ROW="6" REQUIRED="true"
                   SHOW_IF="(DB_VERSION=='MAPR500' OR DB_VERSION=='MAPR510' OR DB_VERSION=='MAPR520')"
                   GROUP="VERSION">
            <DEFAULT>This distribution is deprecated</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="SCHEME" FIELD="CLOSED_LIST"
			NUM_ROW="8" GROUP="SCHEME" REPOSITORY_VALUE="SCHEME">
			<ITEMS DEFAULT="HDFS">
				<ITEM NAME="HDFS" VALUE="HDFS" />
				<ITEM NAME="ADLS" VALUE="ADLS" SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportAzureDataLakeStorage[])" />
				<ITEM NAME="WebHDFS" VALUE="WebHDFS" SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportWebHDFS[])" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="AUTHENTICATION_MODE" FIELD="CLOSED_LIST"
			NUM_ROW="11" SHOW_IF="(DISTRIBUTION=='CUSTOM') AND (CONFIGURATIONS_FROM_CLASSPATH=='false') AND (SCHEME!='ADLS')" REPOSITORY_VALUE="AUTHENTICATION_MODE"
			GROUP="CUSTOM_COMMON_SETTINGS">
			<ITEMS DEFAULT="USERNAME">
				<ITEM NAME="USERNAME" VALUE="USERNAME" />
				<ITEM NAME="KRB" VALUE="KRB" />
				<ITEM NAME="UGI" VALUE="UGI" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="FS_DEFAULT_NAME" FIELD="TEXT"
			REPOSITORY_VALUE="FS_DEFAULT_NAME" REQUIRED="true" NUM_ROW="20" SHOW_IF="(SCHEME!='ADLS') AND (CONFIGURATIONS_FROM_CLASSPATH=='false')">
			<DEFAULT>"hdfs://localhost:9000/"</DEFAULT>
			<DEFAULT IF="(DISTRIBUTION=='MAPR')">"maprfs:///"</DEFAULT>
			<DEFAULT IF="(SCHEME=='HDFS')">"hdfs://localhost:9000/"</DEFAULT>
			<DEFAULT IF="(SCHEME=='WebHDFS')">"webhdfs://localhost:50070/"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONFIGURATIONS_FROM_CLASSPATH" REPOSITORY_VALUE="CONFIGURATIONS_FROM_CLASSPATH" FIELD="CHECK"
			NUM_ROW="22" REQUIRED="true" GROUP="CONFIGURATIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_KRB" FIELD="CHECK" NUM_ROW="25"
			REPOSITORY_VALUE="USE_KRB" GROUP="AUTHENTICATION"
			SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportKerberos[]) AND (CONFIGURATIONS_FROM_CLASSPATH=='false') AND (SCHEME!='ADLS')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NAMENODE_PRINCIPAL" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="NAMENODE_PRINCIPAL" REQUIRED="true" NUM_ROW="26"
			SHOW_IF="(isShow[USE_KRB] AND (USE_KRB=='true')) OR (isShow[AUTHENTICATION_MODE] AND (AUTHENTICATION_MODE=='KRB')) AND (SCHEME!='ADLS')">
			<DEFAULT>"nn/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_KEYTAB" REPOSITORY_VALUE="USE_KEYTAB"
			FIELD="CHECK" NUM_ROW="27"
			SHOW_IF="(isShow[USE_KRB] AND (USE_KRB=='true')) OR (isShow[AUTHENTICATION_MODE] AND (AUTHENTICATION_MODE=='KRB')) AND (SCHEME!='ADLS')"
			GROUP="AUTHENTICATION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PRINCIPAL" REPOSITORY_VALUE="KEYTAB_PRINCIPAL"
			FIELD="TEXT" REQUIRED="true" NUM_ROW="28"
			SHOW_IF="isShow[USE_KEYTAB] AND (USE_KEYTAB=='true') AND (SCHEME!='ADLS')" GROUP="AUTHENTICATION">
			<DEFAULT>"hdfs"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="KEYTAB_PATH" REPOSITORY_VALUE="KEYTAB_PATH"
			FIELD="FILE" REQUIRED="true" NUM_ROW="28"
			SHOW_IF="isShow[USE_KEYTAB] AND (USE_KEYTAB=='true') AND (SCHEME!='ADLS')" GROUP="AUTHENTICATION">
			<DEFAULT>"/tmp/hdfs.headless.keytab"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_MAPRTICKET" FIELD="CHECK" NUM_ROW="29"
            REPOSITORY_VALUE="USE_MAPRTICKET" GROUP="AUTHENTICATION"
            SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportMapRTicket[]) AND (CONFIGURATIONS_FROM_CLASSPATH=='false') AND (SCHEME!='ADLS')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

		
		<PARAMETER NAME="USERNAME" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true" NUM_ROW="30"
			NOT_SHOW_IF="((isShow[USE_KRB] AND (USE_KRB=='true')) OR (isShow[AUTHENTICATION_MODE] AND (AUTHENTICATION_MODE=='KRB'))) OR (CONFIGURATIONS_FROM_CLASSPATH=='true') OR (DISTRIB[DISTRIBUTION, DB_VERSION].doSupportAzureDataLakeStorage[] AND SCHEME=='ADLS')">
			<DEFAULT>"anonymous"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="GROUP" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="GROUP" REQUIRED="true" NUM_ROW="30"
			SHOW_IF="((DISTRIB[DISTRIBUTION, DB_VERSION].doSupportGroup[]) OR (DISTRIBUTION=='CUSTOM' AND AUTHENTICATION_MODE=='UGI')) AND (CONFIGURATIONS_FROM_CLASSPATH=='false') AND (SCHEME!='ADLS')">
			<DEFAULT>"supergroup"</DEFAULT>
		</PARAMETER>
		
	    <PARAMETER NAME="MAPRTICKET_PASSWORD" FIELD="PASSWORD" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPRTICKET_PASSWORD" REQUIRED="true" NUM_ROW="35"
            SHOW_IF="(((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true')) AND (((isShow[USE_KRB]) AND (USE_KRB=='false')) OR (!isShow[USE_KRB]))) AND (SCHEME!='ADLS')">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

          
        <PARAMETER NAME="MAPRTICKET_CLUSTER" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPRTICKET_CLUSTER" REQUIRED="true" NUM_ROW="36"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>"demo.mapr.com"</DEFAULT>
        </PARAMETER>
        
        <PARAMETER NAME="MAPRTICKET_DURATION" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPRTICKET_DURATION" REQUIRED="true" NUM_ROW="38"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>86400L</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="HADOOP_ADVANCED_PROPERTIES"
			REPOSITORY_VALUE="HADOOP_ADVANCED_PROPERTIES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="40" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
				<ITEM NAME="BUILDIN" SHOW_IF="false" VALUE="TRUE" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="USE_DATANODE_HOSTNAME" FIELD="CHECK"
			REPOSITORY_VALUE="USE_DATANODE_HOSTNAME" NUM_ROW="90" REQUIRED="true"
			SHOW_IF="DISTRIB[DISTRIBUTION, DB_VERSION].doSupportUseDatanodeHostname[] AND (SCHEME!='ADLS')">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_HDFS_ENCRYPTION" FIELD="CHECK" GROUP="ENCRYPTION"
			REPOSITORY_VALUE="USE_HDFS_ENCRYPTION" NUM_ROW="100" REQUIRED="true"
			SHOW_IF="DISTRIB[DISTRIBUTION, DB_VERSION].doSupportHDFSEncryption[] AND (SCHEME!='ADLS')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="HDFS_ENCRYPTION_KEY_PROVIDER" FIELD="TEXT" GROUP="ENCRYPTION"
            REPOSITORY_VALUE="HDFS_ENCRYPTION_KEY_PROVIDER" REQUIRED="true" NUM_ROW="110"
            SHOW_IF="((isShow[USE_HDFS_ENCRYPTION]) AND (USE_HDFS_ENCRYPTION=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>"kms://http@localhost:16000/kms"</DEFAULT>
        </PARAMETER>
        
        <!-- ADLS params -->
        <PARAMETER NAME="ADLS_FS_DEFAULT_NAME" FIELD="TEXT"
			REPOSITORY_VALUE="AZURE_FS_DEFAULT_NAME" REQUIRED="true" NUM_ROW="23" 
			SHOW_IF="(SCHEME=='ADLS') AND (CONFIGURATIONS_FROM_CLASSPATH=='false')" GROUP="ADLS">
			<DEFAULT>"adl://account.azuredatalakestore.net"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ADLS_CLIENT_ID" FIELD="TEXT" NUM_ROW="24" REQUIRED="true" 
			SHOW_IF="(SCHEME=='ADLS') AND (CONFIGURATIONS_FROM_CLASSPATH=='false')" GROUP="ADLS">
			<DEFAULT>"XXXX"</DEFAULT>
		</PARAMETER>
			
		<PARAMETER NAME="ADLS_AUTH_TOKEN_ENDPOINT" FIELD="PASSWORD" NUM_ROW="25" REQUIRED="true" 
			SHOW_IF="(SCHEME=='ADLS') AND (CONFIGURATIONS_FROM_CLASSPATH=='false')" GROUP="ADLS">
			<DEFAULT>"https://login.microsoftonline.com/XXX/oauth2/token"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ADLS_CLIENT_KEY" FIELD="PASSWORD" NUM_ROW="26" REQUIRED="true" 
			SHOW_IF="(SCHEME=='ADLS') AND (CONFIGURATIONS_FROM_CLASSPATH=='false')" GROUP="ADLS">
			<DEFAULT>"XXXX"</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
	
        <PARAMETER NAME="SET_MAPR_HOME_DIR" FIELD="CHECK" NUM_ROW="50"
            REPOSITORY_VALUE="SET_MAPR_HOME_DIR" GROUP="AUTHENTICATION"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPR_HOME_DIR" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="MAPR_HOME_DIR" REQUIRED="true" NUM_ROW="50"
            SHOW_IF="((isShow[SET_MAPR_HOME_DIR]) AND (SET_MAPR_HOME_DIR=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>"/opt/mapr"</DEFAULT>
        </PARAMETER>
        
       <PARAMETER NAME="SET_HADOOP_LOGIN" FIELD="CHECK" NUM_ROW="55"
            REPOSITORY_VALUE="SET_HADOOP_LOGIN" GROUP="AUTHENTICATION"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="HADOOP_LOGIN" FIELD="TEXT" GROUP="AUTHENTICATION"
            REPOSITORY_VALUE="HADOOP_LOGIN" REQUIRED="true" NUM_ROW="55"
            SHOW_IF="((isShow[SET_HADOOP_LOGIN]) AND (SET_HADOOP_LOGIN=='true') AND (SCHEME!='ADLS'))">
            <DEFAULT>"kerberos"</DEFAULT>
        </PARAMETER>

	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="reload4j-1.2.19" MODULE="reload4j-1.2.19.jar"
					MVN="mvn:ch.qos.reload4j/reload4j/1.2.19"/>
			<IMPORT NAME="hadoop-conf-kerberos" MODULE="hadoop-conf-kerberos.jar"
				MVN="mvn:org.talend.libraries/hadoop-conf-kerberos/6.0.0"
				UrlPath="platform:/plugin/org.talend.libraries.hadoop.mapr2/lib/hadoop-conf-kerberos.jar"
				REQUIRED_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportKerberos[] AND USE_KRB=='true' AND (DISTRIBUTION!='CUSTOM')) AND (CONFIGURATIONS_FROM_CLASSPATH=='false')" />
			<IMPORT NAME="commons-io" MODULE="commons-io-2.4.jar"
				MVN="mvn:commons-io/commons-io/2.4"
				UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-io-2.4.jar"
				REQUIRED_IF="DISTRIB[DISTRIBUTION, DB_VERSION].doSupportOldImportMode[] AND (DISTRIBUTION=='CLOUDERA') AND (DISTRIBUTION!='CUSTOM')" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS />
</COMPONENT>
