            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPRTICKET_PASSWORD" FIELD="PASSWORD" GROUP="AUTHENTICATION_MAPR"
            REPOSITORY_VALUE="MAPRTICKET_PASSWORD" REQUIRED="true" NUM_ROW="32"
