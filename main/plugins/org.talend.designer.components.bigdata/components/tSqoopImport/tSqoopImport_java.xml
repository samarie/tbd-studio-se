			<DEFAULT>"jdbc:mysql://localhost/database_name"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="38"
			GROUP="COMMON_SQOOP_OPTIONS" REPOSITORY_VALUE="JDBC_PROPERTY/USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD_STORED_IN_FILE" FIELD="CHECK"
			NUM_ROW="39" GROUP="COMMON_SQOOP_OPTIONS"
			SHOW_IF="(USE_COMMANDLINE == 'true' OR (USE_JAVAAPI=='true' AND (DISTRIBUTION=='CUSTOM' OR DISTRIB[DISTRIBUTION, DB_VERSION].doJavaAPISupportStorePasswordInFile[])))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD_FILE" FIELD="TEXT" NUM_ROW="40"
