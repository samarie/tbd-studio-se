			REPOSITORY_VALUE="HADOOP_PROPERTY/USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="JDBC_PROPERTY" FIELD="PROPERTY_TYPE"
			REPOSITORY_VALUE="DATABASE:JDBC" NUM_ROW="36" />

		<PARAMETER NAME="CONNECTION" FIELD="TEXT" NUM_ROW="37"
