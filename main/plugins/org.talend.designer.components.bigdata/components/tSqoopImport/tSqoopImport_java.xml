		<PARAMETER NAME="HADOOP_CODEC" FIELD="TEXT" NUM_ROW="90"
			SHOW_IF="(COMPRESS=='true' AND DEFINE_HADOOP_CODEC=='true')" GROUP="CONTROL_SQOOP_OPTIONS">
