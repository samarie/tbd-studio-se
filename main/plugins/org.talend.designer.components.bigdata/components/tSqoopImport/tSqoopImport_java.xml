		<PARAMETER NAME="SQOOP" FIELD="HADOOP_DISTRIBUTION" NUM_ROW="5"
			SHOW_IF="(USE_JAVAAPI=='true')" GROUP="VERSION" />

		<PARAMETER NAME="HADOOP_CUSTOM_JARS" GROUP="VERSION"
			FIELD="HADOOP_JARS_DIALOG" NUM_ROW="5"
