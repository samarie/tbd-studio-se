		<PARAMETER NAME="DRIVER_CLASS" FIELD="TEXT" NUM_ROW="43"
			REPOSITORY_VALUE="JDBC_PROPERTY/DRIVER_CLASS" REQUIRED="true"
			GROUP="COMMON_SQOOP_OPTIONS" SHOW_IF="USE_JAVAAPI=='true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="TEXT" NUM_ROW="50"
			SHOW_IF="USE_QUERY=='false'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>"table_name"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FILE_FORMAT" FIELD="CLOSED_LIST" NUM_ROW="52"
