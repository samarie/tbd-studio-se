		</PARAMETER>

		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="40"
			GROUP="COMMON_SQOOP_OPTIONS"
			SHOW_IF="PASSWORD_STORED_IN_FILE=='false' OR (USE_JAVAAPI=='true' AND (DISTRIBUTION!='CUSTOM' AND !DISTRIB[DISTRIBUTION, DB_VERSION].doJavaAPISupportStorePasswordInFile[]))"
			REPOSITORY_VALUE="JDBC_PROPERTY/PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DRIVER_JAR" FIELD="TABLE" NUM_ROW="42"
			NB_LINES="2" REPOSITORY_VALUE="JDBC_PROPERTY/DRIVER_JAR" REQUIRED="false"
