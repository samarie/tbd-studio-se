					SHOW_IF="(DISTRIB[DISTRIBUTION, DB_VERSION].doSupportParquetOutput[])" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="DELETE_TARGET_DIR" FIELD="CHECK" NUM_ROW="55"
			GROUP="CONTROL_SQOOP_OPTIONS"
			SHOW_IF="(USE_COMMANDLINE == 'true' OR (USE_JAVAAPI=='true' AND (DISTRIBUTION=='CUSTOM' OR DISTRIB[DISTRIBUTION, DB_VERSION].doJavaAPISqoopImportSupportDeleteTargetDir[])))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="APPEND" FIELD="CHECK" NUM_ROW="60"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COMPRESS" FIELD="CHECK" NUM_ROW="90"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEFINE_HADOOP_CODEC" FIELD="CHECK" NUM_ROW="90"
			SHOW_IF="COMPRESS=='true'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
