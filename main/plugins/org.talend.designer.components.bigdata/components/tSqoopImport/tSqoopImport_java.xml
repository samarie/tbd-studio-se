			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COLUMNS" FIELD="TABLE" NUM_ROW="105"
			NB_LINES="3" SHOW_IF="(USE_COLUMNS=='true' AND USE_QUERY=='false')"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<ITEMS>
				<ITEM NAME="COLUMN" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="USE_WHERE" FIELD="CHECK" NUM_ROW="110"
			SHOW_IF="USE_QUERY=='false'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="WHERE" FIELD="TEXT" NUM_ROW="110"
			SHOW_IF="(USE_WHERE=='true' AND USE_QUERY=='false')" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>"id&gt;0"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_QUERY" FIELD="CHECK" NUM_ROW="115"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="QUERY" FIELD="MEMO_SQL" NUM_ROW="120"
			SHOW_IF="USE_QUERY=='true'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>"select id, name from employee where $CONDITIONS"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="QUERY_LABEL" FIELD="LABEL" NUM_ROW="125"
			SHOW_IF="USE_QUERY=='true'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>When importing a free-form query, you must specify a
				destination directory. If you want to import the results of a query
				in parallel, you must select a splitting column with --split-by
			</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_TARGET" FIELD="CHECK" NUM_ROW="130"
			SHOW_IF="USE_QUERY=='false'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET" FIELD="TEXT" NUM_ROW="130"
			SHOW_IF="(USE_TARGET=='true') or (USE_QUERY=='true')" GROUP="CONTROL_SQOOP_OPTIONS">
